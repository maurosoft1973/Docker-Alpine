{
    "Serilog": {
        "MinimumLevel": {
            "Override": {
                "Microsoft": "Information",
                "System": "Warning",
                "Polly": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] Alpine Automatic Build {Message:lj} {NewLine}{Exception}"
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "log.txt",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": "1",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] Alpine Automatic Build {Message:lj} {NewLine}{Exception}",
                    "shared": "true"
                }
            }
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System.Net.Http.HttpClient": "Warning",
            "ZiggyCreatures.Caching.Fusion": "Warning",
            "Polly": "Warning"
        }
    },
    "ARCHITECTURES": "x86_64,aarch64",
    "IMAGE_NAME": "alpinenew",
    "DO_PUSH": "true",
    "HTTP_USER_AGENT": "Alpine.AutomaticBuild",
    "DOCKERFILE_PATH": "",
    "ENABLE_PARALLEL_BUILDS": false,
    "MAX_PARALLEL_BUILDS": 2,
    "ENABLE_HEALTH_CHECKS": true,
    "ENABLE_VULN_SCAN": false,
    "VULN_BLOCKING_SEVERITY": "CRITICAL"
}
